*.font: xft:Overpass Mono:size=10

Xft.dpi:                        96
Xft.antialias:                  1
Xft.autohint:                   1
Xft.hinting:                    1
Xft.hintstyle:                  hintslight
Xft.lcdfilter:                  lcdlight
Xft.rgba:                       rgb

URxvt*termName:                 rxvt-256color
URxvt.depth:                    32
URxvt*lineSpace:                1
URxvt.letterSpace:              -1
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt*geometry:                 65x17
URxvt.scrollBar:                false
URxvt*loginShell:               true
URxvt.internalBorder:           25
URxvt.perl-ext:	 		default,matcher
URxvt.url-launcher:             firefox
URxvt.matcher.button: 		1

!! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: urxvt -e lynx -cfg=~/.lynx.cfg -lss=~/.lynx.lss
URxvt.url-select.underline: true

!! resize-font
URxvt.resize-font.smaller: C-j
URxvt.resize-font.bigger:  C-k

!!clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste

!! Input method
URxvt.inputMethod: ibus
URxvt.preeditType: OverTheSpot

! X colors.
! Generated by 'wal'
*foreground:        #dddee0
*background:        #1a2439
*.foreground:       #dddee0
*.background:       #1a2439
emacs*foreground:   #dddee0
emacs*background:   #1a2439
URxvt*foreground:   #dddee0
XTerm*foreground:   #dddee0
UXTerm*foreground:  #dddee0
URxvt*background:   [100]#1a2439
XTerm*background:   #1a2439
UXTerm*background:  #1a2439
URxvt*cursorColor:  #dddee0
XTerm*cursorColor:  #dddee0
UXTerm*cursorColor: #dddee0
URxvt*borderColor:  [100]#1a2439

